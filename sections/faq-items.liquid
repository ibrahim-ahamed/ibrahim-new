<section id="section-tamplet-{{ section.id }}">
  <div class="container">
    <div class="row">
      {% for block in section.blocks %}
      <div class="col-md-{{ section.settings.columes }}">
        <div class="faq-inner">
          <h2>{{ block.settings.heading_title }}</h2>
          <div>
            {{ block.settings.heading_title }}
          </div>
        </div>
      </div>
      {% endfor %}
    </div>
  </div>
</section>


{% schema %}
  {
	"name": "Footer",
	"settings": [

		{
			"type": "range",
			"id": "columes",
			"default": 1,
			"min": 1,
			"max": 4,
			"label": "Width Colume Fix"

		}
	],

      "name": "Footer",
          "settings": [

              {
                  "type": "range",
                  "id": "columes",
                  "default": 3,
                  "min": 1,
                  "max": 4,
                  "label": "Width Colume Fix"

              }
	],

    "name": "FAQ",
    "blocks": [
		{
			"name": "Faq Item",
			"type": "faqww",
			"limit": 4,
			"settings": [
				{
					"type": "text",
					"id": "heading_title",
					"label": "Title",
					"default": "hqwsqahsahsh"
				},
				{
					"type": "richtext",
					"id": "heading_description",
					"label": "Description",
					"default": "<p>hhqwsqahsahshhqwsqahsahshhqwsqahsahshhqwsqahsahshhqwsqahsahshqwsqahsahsh</p>"
				}
			] 
		}
	],
	"presets": [
                {
                "name": "FAQ",
                "category": "faq"
                }
                ]
  }
{% endschema %}
